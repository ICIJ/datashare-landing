{
  "name": "datashare-landing",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vite dev",
    "build": "vite build",
    "test:unit": "vitest --run",
    "test:unit:watch": "vitest",
    "lint": "eslint",
    "deploy": "node deploy.js",
    "predeploy": "yarn build"
  },
  "dependencies": {
    "@fortawesome/free-brands-svg-icons": "^6.1.2",
    "@fortawesome/free-solid-svg-icons": "^6.1.2",
    "@icij/murmur": "3.2.6",
    "axios": "^0.26.0",
    "bootstrap": "4",
    "bootstrap-vue": "^2.21.2",
    "js-cookie": "^2.2.1",
    "ua-parser-js": "^1.0.2",
    "vue": "^2.6.14",
    "vue-i18n": "^8.26.7"
  },
  "devDependencies": {
    "@vitejs/plugin-vue2": "^2.2.0",
    "@vue/test-utils": "^1.1.3",
    "autoprefixer": "^10.4.13",
    "chalk": "^4.1.2",
    "eslint": "^7.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "gh-pages": "^2.2.0",
    "jsdom": "^21.1.0",
    "sass": "^1.54.0",
    "vite": "^4.1.1",
    "vitest": "^0.28.4",
    "vue-template-compiler": "^2.7.8"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
